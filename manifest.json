{
  "manifest_version": 2,
  "name": "疫距數得_老師端",
  "description": "疫距數得_老師端",
  "version": "1.0.0",
  "icons": {
    "16": "icon/icon.png",
    "48": "icon/icon.png",
    "128": "icon/icon.png"
  },
  "browser_action": {
    "default_icon": "icon/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "jquery.min.js","axios.min.js","execute.js"
      ]
    }
  ]
}